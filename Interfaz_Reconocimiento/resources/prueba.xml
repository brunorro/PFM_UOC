			usuarioJetos
		</param>
		<param name="dbPassword">
			passwordJetos
		</param>
		</accessDatabase>
	
	
	<biometricDatabase>
		<dbDriver>
			SQLITE
		</dbDriver>
		<param name="dbFile">
			../BBDD_Caracteristicas/BDCaracteristicas.bd
		</param>
	</biometricDatabase>

	<recoPreferences>
		<Gray>0</Gray>
		<ShowFaces>1</ShowFaces>
		<ShowFeatures>1</ShowFeatures>
	</recoPreferences>
	

